<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(registration_session) -%>">
  <span class='title'>
    <%= registration_session.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), registration_session_path(registration_session),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_registration_session_path(registration_session),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_registration_session_path(registration_session),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => registration_session.title),
        :method => :delete %>
  </span>
</li>
