<ul>
  <% if Admin::RegistrationSessionsController.searchable? %>
    <li>
      <%= render :partial => "/shared/admin/search",
                  :locals => {
                    :url => admin_registration_sessions_path
                  } %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), new_admin_registration_session_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? and Admin::RegistrationSessionsController.sortable? and RegistrationSession.count > 1 %>
  <li>
    <%= link_to t('.reorder', :what => "Registration Sessions"),
                 admin_registration_sessions_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Registration Sessions"),
                 admin_registration_sessions_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>