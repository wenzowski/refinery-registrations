<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(educational_stage) -%>">
  <span class='title'>
    <%= educational_stage.name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), educational_stage_path(educational_stage),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_educational_stage_path(educational_stage),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_educational_stage_path(educational_stage),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => educational_stage.name),
        :method => :delete %>
  </span>
</li>
