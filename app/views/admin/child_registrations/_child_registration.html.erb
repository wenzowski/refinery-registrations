<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(child_registration) -%>">
  <span class='title'>
    <%= child_registration.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), child_registration_path(child_registration),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_child_registration_path(child_registration),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_child_registration_path(child_registration),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => child_registration.title),
        :method => :delete %>
  </span>
</li>
